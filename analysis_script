data = quick_pick_experiment_raw_exposed_non_exposed
data$exp_var = factor(data$exp_var, ordered = FALSE)
summary(data)

boxplot(listening_per_user ~ exp_var, data = data)
boxplot(listener_rate ~ exp_var, data = data)
boxplot(listener ~ exp_var, data = data)
boxplot(total_plays ~ exp_var, data = data)

wilcox.test(listening_per_user~exp_var, data = data, alternative = "two.sided", paired = FALSE)
wilcox.test(listener_rate~exp_var, data = data, alternative = "two.sided", paired = FALSE)
wilcox.test(listener~exp_var, data = data, alternative = "two.sided", paired = FALSE)
wilcox.test(total_plays~exp_var, data = data, alternative = "two.sided", paired = FALSE)
